      xmlns:five="http://namespaces.zope.org/five"
      i18n_domain="ftwbook.graphicblock">

    <class class=".graphicblock.GraphicBlock">

        <require
              permission="zope2.View"
              interface="..interfaces.IGraphicBlock"
              />

        <require
              permission="cmf.ModifyPortalContent"
